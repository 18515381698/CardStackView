<?xml version="1.0" encoding="utf-8"?>

<resources>
    <declare-styleable name="CardStackView">
        <attr name="visibleCardCount" format="integer"/>
        <attr name="swipeThreshold" format="integer"/>
        <attr name="stackFrom" format="enum">
            <enum name="bottom" value="0"/>
            <enum name="top" value="1"/>
        </attr>
        <attr name="isElevationEnabled" format="boolean"/>
    </declare-styleable>
</resources>